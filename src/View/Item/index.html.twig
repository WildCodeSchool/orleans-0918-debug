{% extends 'layout.twig' %}

{% block content %}
    <h1>Items</h1>
    <ul>
        {% for items in item %}
            <li><a href="/item/{% item.id %}">{% item.id %} {% item.name %} </li></a>
        {% else %}
            <li>Nothing to display</li>
        {% endfor %}
    </ul>
    <a href="/item/add">Ajouter</a>
{% endblock %}
